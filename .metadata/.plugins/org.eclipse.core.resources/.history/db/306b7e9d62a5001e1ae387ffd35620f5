package com.basic;

public class Pattern5 {
	/*
	 * a 
	 * b b 
	 * c c c 
	 * d d d d
	 * e e e e e
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char ch = 'a';
		for(int row = 1;row <=5;row++) {
			for(int col = 1;col<=row;col++) {
				System.out.println(ch + " ");
			}
			ch++;
		}
	}

}
